<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>run</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:3.1.0:exec</goal>
        </goals>
        <properties>
            <exec.vmArgs>-Xlog:disable --add-opens java.base/java.lang=ALL-UNNAMED --add-opens java.base/sun.misc=ALL-UNNAMED</exec.vmArgs>
            <exec.executable>java</exec.executable>
            <exec.args>-classpath %classpath ${exec.vmArgs} ${exec.mainClass} ${exec.appArgs}</exec.args>
            <exec.mainClass>${exec.mainClass}</exec.mainClass>
        </properties>
    </action>
</actions>